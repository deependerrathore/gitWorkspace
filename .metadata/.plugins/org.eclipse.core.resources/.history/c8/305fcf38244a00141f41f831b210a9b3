package tk.socialstu.objects;

public class Storage {
	
	private final int size;	//size of the disk
	private final String type;//type of the disk like SSD ,HDD
	
	public Storage(int size, String type) {
		this.size = size;
		this.type = type;
	}
	
	public int getSize() {
		return size;
	}
	
	public String getType() {
		return type;
	}

	
}
