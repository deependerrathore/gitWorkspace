package tk.socialstu.objects;
//This is the disk class
public class Disk {
	private final int size;	//size of the disk
	private final String type;//type of the disk like SSD ,HDD
	
	public Disk(int size, String type) {
		this.size = size;
		this.type = type;
	}
	
	public int getSize() {
		return size;
	}
	
	public String getType() {
		return type;
	}

	
}
